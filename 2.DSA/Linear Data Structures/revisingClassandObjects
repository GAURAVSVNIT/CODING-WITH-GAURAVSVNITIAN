#include<iostream>
using namespace std;
class student
{
    public:
    string name;
    int age;
    int rollno;
    float marks;
    student(string name,int a,int r,float m)
    {
        this->name=name;
        age=a;
        rollno=r;
        marks=m;
        cout<<"constructor called"<<endl;
    }
}; 
int main()
{
    student s1("gaurav",20,1,50);
